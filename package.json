{
	"name": "rest-countries-api-with-color-theme-switcher-master",
	"version": "1.0.0",
	"description": "Design preview for the REST Countries API with color theme switcher coding challenge",
	"main": "src/index.html",
	"scripts": {
		"start": "parcel src/index.html",
		"build": "parcel build src/index.html"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@parcel/transformer-sass": "^2.0.0-beta.2",
		"autoprefixer": "^10.2.6",
		"parcel": "^2.0.0-beta.2",
		"parcel-plugin-static-files-copy": "file:../..",
		"postcss": "^8.3.5",
		"sass": "^1.35.1"
	},
	"dependencies": {
		"@babel/polyfill": "^7.12.1",
		"core-js": "^3.15.2",
		"path": "^0.12.7",
		"regenerator-runtime": "^0.13.7"
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {
				"overrideBrowserslist": [
					"> 0.25%",
					"last 6 versions",
					"ie >= 11"
				],
				"grid": true
			}
		}
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env"
			]
		]
	},
	"browserslist": [
		"> 0.25%",
		"not dead"
	],
	"staticFiles": {
		"staticPath": [
			{
				"staticPath": "src/img",
				"staticOutDir": "dist/img"
			}
		],
		"watcherGlob": "**"
	}
}
